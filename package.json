{
  "name": "@ivujs/i-utils",
  "moduleName": "iUtils",
  "version": "2.1.0",
  "type": "module",
  "description": "前端模块化 JavaScript 工具库",
  "author": "<gao911222@163.com>",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.mjs",
  "unpkg": "dist/lib/index.full.umd.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./es": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.mjs"
    },
    "./cjs": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver/index.d.ts",
      "import": "./dist/resolver/index.mjs",
      "require": "./dist/resolver/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsx ./build/build.ts",
    "build:docs": "typedoc",
    "build:all": "pnpm build && pnpm build:docs"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.9",
    "chalk": "^5.6.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "execa": "^9.6.1",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "rollup": "^4.55.1",
    "rollup-plugin-dts": "^6.3.0",
    "ts-morph": "^27.0.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-auto-import": "^21.0.0",
    "vite": "^7.3.1"
  },
  "keywords": [
    "i-utils",
    "iUtils",
    "js utils",
    "ts utils",
    "js 工具库",
    "ts 工具库",
    "前端工具库",
    "模块化工具库",
    "JavaScript 工具库",
    "TypeScript 工具库"
  ],
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/qq575792372/i-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/qq575792372/i-utils.git"
  },
  "bugs": {
    "url": "https://github.com/qq575792372/i-utils/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
