{
  "entryPoints": ["./src/*/index.ts"],
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["./src/index.ts", "./src/resolver/*.ts"],
  "categorizeByGroup": true,
  "outputFileStrategy": "modules",
  "excludePrivate": true,
  "cleanOutputDir": true,
  "sort": ["source-order"],
  "skipErrorChecking": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "expandObjects": true,
  "expandParameters": true,
  "disableSources": true,
  "useCodeBlocks": true,
  "readme": "none",
  "lang": "zh"
}
